{
  "name": "vscode-kaoto",
  "publisher": "redhat",
  "displayName": "Kaoto",
  "description": "Kaoto - No Code and low code Integration editor",
  "version": "0.3.0",
  "preview": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaotoIO/vscode-kaoto.git"
  },
  "bugs": "https://github.com/KaotoIO/vscode-kaoto/issues",
  "homepage": "https://github.com/KaotoIO/vscode-kaoto",
  "author": {
    "name": "Aur√©lien Pupier",
    "email": "apupier@redhat.com"
  },
  "scripts": {
    "lint": "yarn eslint ./src --ext .ts,.tsx",
    "postinstall": "node ./scripts/postinstall.js",
    "build:prod": "rimraf dist && webpack && yarn pack:prod",
    "build:dev": "rimraf dist && webpack --env dev",
    "pack:prod": "vsce package --githubBranch main --no-dependencies --yarn -o ./dist/vscode_kaoto_$npm_package_version.vsix",
    "compile": "webpack",
    "watch": "webpack --env dev",
    "run:webmode": "yarn vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --open-devtools ./resources",
    "test:it": "yarn test:it:clean && tsc --project tsconfig.it-tests.json --skipLibCheck --sourceMap true && extest setup-and-run --yarn --uninstall_extension --extensions_dir ./test-resources out/*.test.js",
    "test:it:clean": "rimraf ./test-resources && rimraf ./out && rimraf *.vsix"
  },
  "dependencies": {
    "@kaoto/kaoto-ui": "0.5.0",
    "@kie-tools-core/backend": "^0.25.0",
    "@kie-tools-core/editor": "^0.25.0",
    "@kie-tools-core/i18n": "^0.25.0",
    "@kie-tools-core/monaco-editor": "^0.25.0",
    "@kie-tools-core/patternfly-base": "^0.25.0",
    "@kie-tools-core/vscode-extension": "^0.25.0",
    "@kie-tools-core/workspace": "^0.25.0",
    "@patternfly/react-core": "4.250.1",
    "@redhat-developer/vscode-redhat-telemetry": "^0.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "icon": "icon.png",
  "main": "./dist/extension/extension.js",
  "browser": "./dist/extension/extensionWeb.js",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Kaoto",
    "Apache Camel",
    "Low Code Integration",
    "No Code Integration"
  ],
  "activationEvents": [
    "onCustomEditor:webviewEditorsKaoto",
    "workspaceContains:**/*.kaoto.yaml",
    "workspaceContains:**/*.kaoto.yml",
    "onStartupFinished"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    }
  },
  "contributes": {
    "configuration": {
      "properties": {
        "redhat.telemetry.enabled": {
          "type": "boolean",
          "default": null,
          "markdownDescription": "Enable usage data and errors to be sent to Red Hat servers. Read our [privacy statement](https://developers.redhat.com/article/tool-data-collection).",
          "tags": [
            "telemetry"
          ],
          "scope": "window"
        }
      }
    },
    "customEditors": [
      {
        "viewType": "webviewEditorsKaoto",
        "displayName": "Kaoto Editor",
        "selector": [
          {
            "filenamePattern": "*.{kaoto.yaml,kaoto.yml}"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "kaoto-yaml",
        "extensions": [
          ".kaoto.yml",
          ".kaoto.yaml"
        ],
        "aliases": [
          "Kaoto (YAML)",
          "kaoto.yaml",
          "kaoto.yml"
        ]
      }
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/react": "18.0.18",
    "@vscode/test-web": "^0.0.32",
    "buffer": "^6.0.3",
    "chai": "^4.3.7",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "mocha": "^10.1.0",
    "node-fetch": "2",
    "npm-link-shared": "^0.5.6",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "vsce": "^2.11.0",
    "vscode-extension-tester": "^5.1.0",
    "web-worker": "^1.2.0",
    "webpack": "5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}

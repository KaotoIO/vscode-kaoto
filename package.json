{
  "name": "vscode-kaoto",
  "publisher": "redhat",
  "displayName": "Kaoto",
  "description": "Kaoto - The Integration Designer for Apache Camel",
  "version": "2.10.0",
  "federatedModuleName": "kaoto",
  "preview": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaotoIO/vscode-kaoto.git"
  },
  "bugs": "https://github.com/KaotoIO/vscode-kaoto/issues",
  "homepage": "https://kaoto.io/",
  "author": {
    "name": "AurÃ©lien Pupier",
    "email": "apupier@redhat.com"
  },
  "scripts": {
    "lint": "eslint src it-tests",
    "build:prod": "rimraf dist && yarn run compile && yarn run lint",
    "build:dev": "rimraf dist && yarn run watch && yarn run lint",
    "build:test:it": "rimraf ./out && tsc --project tsconfig.it-tests.json --skipLibCheck --sourceMap true && yarn run lint",
    "build:test:unit": "rimraf ./out && tsc --project tsconfig.unit-tests.json --skipLibCheck --sourceMap true && yarn run lint",
    "compile": "webpack",
    "watch": "webpack --env dev",
    "run:webmode": "yarn vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --open-devtools ./resources",
    "test:unit": "yarn build:test:unit && vscode-test",
    "test:it": "yarn build:test:it && extest setup-and-run --yarn --uninstall_extension --extensions_dir ./test-resources --open_resource './test Fixture with speci@l chars' 'out/**/*.test.js'",
    "setup:test:it:with-prebuilt-vsix": "yarn build:test:it && extest get-vscode && extest get-chromedriver && extest install-vsix --vsix_file vscode-kaoto-$npm_package_version.vsix --extensions_dir ./test-resources && extest run-tests --uninstall_extension --extensions_dir ./test-resources --open_resource './test Fixture with speci@l chars'",
    "test:it:with-prebuilt-vsix": "yarn setup:test:it:with-prebuilt-vsix --code_settings ./it-tests/vscode-settings.json 'out/01_env/set.camel.version.js' 'out/**/*.test.js' 'out/01_env/check.camel.version.js'",
    "test:it:with-prebuilt-vsix:minikube": "yarn setup:test:it:with-prebuilt-vsix --code_settings ./it-tests/vscode-settings-minikube.json 'out/01_env/set.camel.version.js' 'out/**/*.test.js' 'out/01_env/check.camel.version.js'",
    "test:it:clean": "rimraf ./test-resources && rimraf ./out && rimraf *.vsix"
  },
  "dependencies": {
    "@kaoto/kaoto": "2.9.0",
    "@kie-tools-core/backend": "10.0.0",
    "@kie-tools-core/editor": "10.0.0",
    "@kie-tools-core/i18n": "10.0.0",
    "@kie-tools-core/vscode-extension": "10.0.0",
    "@patternfly/patternfly": "6.4.0",
    "@patternfly/react-code-editor": "6.4.0",
    "@patternfly/react-core": "6.4.0",
    "@patternfly/react-icons": "6.4.0",
    "@patternfly/react-table": "6.4.0",
    "@patternfly/react-topology": "6.4.0",
    "@redhat-developer/vscode-extension-proposals": "^0.0.23",
    "@redhat-developer/vscode-redhat-telemetry": "^0.10.2",
    "compare-versions": "^6.1.1",
    "get-port-please": "^3.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-monaco-editor": "0.56.2",
    "valid-filename": "4.0.0",
    "wait-on": "^9.0.3",
    "yaml": "^2.8.2"
  },
  "icon": "icon.png",
  "main": "./dist/extension/extension.js",
  "browser": "./dist/extension/extensionWeb.js",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Kaoto",
    "Apache Camel",
    "Low Code Integration",
    "No Code Integration"
  ],
  "activationEvents": [
    "workspaceContains:**/*.camel.yaml",
    "workspaceContains:**/*.camel.yml",
    "workspaceContains:**/*.kamelet.yaml",
    "workspaceContains:**/*.kamelet.yml",
    "workspaceContains:**/*.pipe.yaml",
    "workspaceContains:**/*.pipe.yml",
    "workspaceContains:**/*-pipe.yaml",
    "workspaceContains:**/*-pipe.yml",
    "workspaceContains:**/*.camel.xml",
    "onStartupFinished"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    },
    "virtualWorkspaces": {
      "supported": "limited",
      "description": "In virtual workspaces, actions which requires Camel JBang CLI cannot be working because of missing Shell Execution environment."
    }
  },
  "contributes": {
    "icons": {
      "export-icon": {
        "description": "Export icon",
        "default": {
          "fontPath": "./icons/integrations/export-icons.woff",
          "fontCharacter": "\\E900"
        }
      }
    },
    "walkthroughs": [
      {
        "id": "kaoto.walkthrough",
        "title": "Kaoto first steps",
        "description": "Let's start with Kaoto Integration Designer",
        "icon": "icons/kaoto.png",
        "steps": [
          {
            "id": "kaoto.walkthrough.step0",
            "title": "Open Kaoto view",
            "description": "Click the **Kaoto icon** in the Activity Bar to open the Kaoto view.\n\n[Open Kaoto View](command:workbench.view.extension.kaoto-view)",
            "media": {
              "image": "images/walkthrough/activity-bar.png",
              "altText": "Kaoto Activity Bar icon"
            },
            "completionEvents": [
              "onView:kaoto.integrations",
              "onView:kaoto.deployments",
              "onView:kaoto.help",
              "onCommand:workbench.view.extension.kaoto-view"
            ]
          },
          {
            "id": "kaoto.walkthrough.step1",
            "title": "Open workspace",
            "description": "Open an existing workspace or create a new one.\n\n[Open Workspace](command:vscode.openFolder)",
            "media": {
              "image": "images/walkthrough/open-workspace.png",
              "altText": "Open Workspace"
            },
            "when": "workspaceFolderCount == 0",
            "completionEvents": [
              "onWorkspaceOpen",
              "onCommand:workbench.action.openWorkspace",
              "onCommand:vscode.openFolder"
            ]
          },
          {
            "id": "kaoto.walkthrough.step2",
            "title": "Create a new Camel Route",
            "description": "Click the ``New File...`` button inside Integrations view to create a new Camel Route.\n\n[New Camel Route...](command:kaoto.new.camel.file)",
            "media": {
              "image": "images/walkthrough/new-camel-route.png",
              "altText": "New Camel Route"
            },
            "completionEvents": [
              "onCommand:kaoto.new.camel.file",
              "onCommand:kaoto.camel.jbang.init.route",
              "onCommand:kaoto.camel.jbang.init.kamelet.yaml",
              "onCommand:kaoto.camel.jbang.init.pipe.yaml"
            ]
          },
          {
            "id": "kaoto.walkthrough.step3",
            "title": "Run the Camel Route",
            "description": "Click the ``Run`` button inside Integrations view to run the Camel Route.",
            "media": {
              "image": "images/walkthrough/run-camel-route.png",
              "altText": "Run Camel Route"
            },
            "completionEvents": [
              "onCommand:kaoto.integrations.run"
            ]
          },
          {
            "id": "kaoto.walkthrough.step4",
            "title": "Pause the Camel Route",
            "description": "Click the ``Pause`` button inside Deployments view to pause the Camel Route.",
            "media": {
              "image": "images/walkthrough/pause-camel-route.png",
              "altText": "Pause Camel Route"
            },
            "completionEvents": [
              "onCommand:kaoto.deployments.route.suspend"
            ]
          },
          {
            "id": "kaoto.walkthrough.step5",
            "title": "Resume the Camel Route",
            "description": "Click the ``Resume`` button inside Deployments view to resume the Camel Route.",
            "media": {
              "image": "images/walkthrough/resume-camel-route.png",
              "altText": "Resume Camel Route"
            },
            "completionEvents": [
              "onCommand:kaoto.deployments.route.resume"
            ]
          },
          {
            "id": "kaoto.walkthrough.step6",
            "title": "Terminate the Camel Route",
            "description": "Click the ``Terminate`` button inside Deployments view to terminate the Camel Route.",
            "media": {
              "image": "images/walkthrough/terminate-camel-route.png",
              "altText": "Terminate Camel Route"
            },
            "completionEvents": [
              "onCommand:kaoto.deployments.stop"
            ]
          },
          {
            "id": "kaoto.walkthrough.step7",
            "title": "Show the Camel Route source code",
            "description": "Click the ``Show Source`` button inside Integrations view or click the Source Code icon in the editor title to show the Camel Route source code.",
            "media": {
              "image": "images/walkthrough/show-source-code.png",
              "altText": "Show Camel Route Source Code"
            },
            "completionEvents": [
              "onCommand:kaoto.integrations.showSource",
              "onCommand:kaoto.open.source"
            ]
          },
          {
            "id": "kaoto.walkthrough.step8",
            "title": "Explore extension settings",
            "description": "Explore extension settings to customize Kaoto behavior.\n\n[Open Kaoto Settings](command:workbench.action.openSettings?%5B%22@ext:redhat.vscode-kaoto%22%5D)",
            "media": {
              "image": "images/walkthrough/extension-settings.png",
              "altText": "Explore Extension Settings"
            },
            "completionEvents": [
              "onCommand:workbench.action.openSettings"
            ]
          },
          {
            "id": "kaoto.walkthrough.step9",
            "title": "Check out the documentation",
            "description": "Check out the documentation to learn more about Kaoto.\n\n[Open Documentation](https://kaoto.io/docs/manual/)",
            "media": {
              "image": "images/logo-kaoto.png",
              "altText": "Check out the Documentation"
            },
            "completionEvents": [
              "onLink:https://kaoto.io/docs/manual/"
            ]
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "kaoto.whatsNew.show",
        "title": "Show What's New",
        "category": "Kaoto"
      },
      {
        "command": "kaoto.open",
        "title": "Open with Kaoto Graphical Editor for Camel"
      },
      {
        "command": "kaoto.open.source",
        "title": "Open Source Code",
        "shortTitle": "Show Source",
        "enablement": "activeCustomEditorId == webviewEditorsKaoto",
        "category": "Kaoto",
        "icon": {
          "dark": "./icons/commands/source_inactive_dark.png",
          "light": "./icons/commands/source_inactive_light.png"
        }
      },
      {
        "command": "kaoto.close.source",
        "title": "Close Source Code",
        "shortTitle": "Close Source",
        "enablement": "(resourceFilename =~ /\\.(camel|kamelet|pipe).yaml$/ || resourceFilename =~ /\\-pipe.yaml$/ || resourceFilename =~ /\\.camel.xml$/)",
        "category": "Kaoto",
        "icon": "./icons/commands/source_active.png"
      },
      {
        "command": "kaoto.new.camel.file",
        "title": "Camel File...",
        "category": "Kaoto",
        "enablement": "false",
        "icon": "$(new-file)"
      },
      {
        "command": "kaoto.camel.jbang.init.route",
        "shortTitle": "New Camel Route...",
        "title": "Create a Camel Route using YAML DSL",
        "category": "Kaoto"
      },
      {
        "command": "kaoto.camel.jbang.init.kamelet.yaml",
        "shortTitle": "New Kamelet...",
        "title": "Create a Kamelet using YAML DSL",
        "category": "Kaoto"
      },
      {
        "command": "kaoto.camel.jbang.init.pipe.yaml",
        "shortTitle": "New Pipe...",
        "title": "Create a Custom Resource Pipe using YAML DSL",
        "category": "Kaoto"
      },
      {
        "command": "kaoto.camel.jbang.export",
        "title": "Export: File",
        "category": "Kaoto",
        "icon": "$(export-icon)"
      },
      {
        "command": "kaoto.camel.jbang.export.folder",
        "title": "Export: Folder",
        "category": "Kaoto",
        "icon": "$(export-icon)"
      },
      {
        "command": "kaoto.camel.jbang.export.workspace",
        "title": "Export: Workspace",
        "category": "Kaoto",
        "icon": "$(export-icon)"
      },
      {
        "command": "kaoto.integrations.kubernetes.run",
        "title": "Deploy: File",
        "category": "Kaoto",
        "icon": {
          "dark": "./icons/kubernetes/kube-dark.png",
          "light": "./icons/kubernetes/kube-light.png"
        }
      },
      {
        "command": "kaoto.integrations.refresh",
        "title": "Refresh",
        "category": "Kaoto",
        "icon": "$(refresh)"
      },
      {
        "command": "kaoto.integrations.showSource",
        "title": "Show Source",
        "category": "Kaoto",
        "icon": "$(code)"
      },
      {
        "command": "kaoto.integrations.delete",
        "title": "Delete",
        "category": "Kaoto",
        "icon": "$(trash)"
      },
      {
        "command": "kaoto.integrations.updateDependencies",
        "title": "Update Camel Dependencies",
        "shortTitle": "Update Dependencies",
        "category": "Kaoto",
        "icon": "$(sync)"
      },
      {
        "command": "kaoto.integrations.run",
        "title": "Run: File",
        "category": "Kaoto",
        "icon": {
          "dark": "./icons/kaoto-run/kaoto-run-dark.png",
          "light": "./icons/kaoto-run/kaoto-run-light.png"
        }
      },
      {
        "command": "kaoto.integrations.run.workspace",
        "title": "Run: Workspace",
        "category": "Kaoto",
        "icon": "$(run-all)"
      },
      {
        "command": "kaoto.integrations.run.all.workspaces",
        "title": "Run: All Workspaces",
        "category": "Kaoto",
        "icon": "$(run-all)"
      },
      {
        "command": "kaoto.integrations.run.folder",
        "title": "Run: Folder",
        "category": "Kaoto",
        "icon": "$(play)"
      },
      {
        "command": "kaoto.deployments.refresh",
        "title": "Refresh",
        "category": "Kaoto",
        "icon": "$(refresh)"
      },
      {
        "command": "kaoto.deployments.stop",
        "title": "Terminate",
        "category": "Kaoto",
        "icon": "$(circle-slash)"
      },
      {
        "command": "kaoto.deployments.logs",
        "title": "Follow Logs",
        "category": "Kaoto",
        "icon": "$(terminal)"
      },
      {
        "command": "kaoto.deployments.route.start",
        "title": "Start",
        "category": "Kaoto",
        "icon": "$(debug-start)"
      },
      {
        "command": "kaoto.deployments.route.resume",
        "title": "Resume",
        "category": "Kaoto",
        "icon": "$(debug-continue)"
      },
      {
        "command": "kaoto.deployments.route.suspend",
        "title": "Suspend",
        "category": "Kaoto",
        "icon": "$(debug-pause)"
      },
      {
        "command": "kaoto.deployments.route.stop",
        "title": "Stop",
        "category": "Kaoto",
        "icon": "$(debug-stop)"
      },
      {
        "command": "kaoto.undo",
        "title": "Undo",
        "category": "Kaoto"
      },
      {
        "command": "kaoto.redo",
        "title": "Redo",
        "category": "Kaoto"
      }
    ],
    "keybindings": [
      {
        "command": "kaoto.open.source",
        "key": "ctrl+k v",
        "mac": "cmd+k v",
        "when": "activeCustomEditorId == webviewEditorsKaoto"
      },
      {
        "command": "kaoto.undo",
        "key": "ctrl+z",
        "mac": "cmd+z",
        "when": "activeCustomEditorId == webviewEditorsKaoto"
      },
      {
        "command": "kaoto.redo",
        "key": "ctrl+shift+z",
        "mac": "cmd+shift+z",
        "when": "activeCustomEditorId == webviewEditorsKaoto"
      },
      {
        "command": "kaoto.redo",
        "key": "ctrl+y",
        "mac": "cmd+shift+z",
        "when": "activeCustomEditorId == webviewEditorsKaoto"
      }
    ],
    "menus": {
      "kaoto.new.file": [
        {
          "command": "kaoto.camel.jbang.init.route",
          "group": "kaoto@1"
        },
        {
          "command": "kaoto.camel.jbang.init.kamelet.yaml",
          "group": "kaoto@2"
        },
        {
          "command": "kaoto.camel.jbang.init.pipe.yaml",
          "group": "kaoto@3"
        }
      ],
      "commandPalette": [
        {
          "command": "kaoto.open",
          "when": "false"
        },
        {
          "command": "kaoto.open.source",
          "when": "false"
        },
        {
          "command": "kaoto.close.source",
          "when": "false"
        },
        {
          "command": "kaoto.camel.jbang.init.route",
          "when": "false"
        },
        {
          "command": "kaoto.camel.jbang.init.kamelet.yaml",
          "when": "false"
        },
        {
          "command": "kaoto.camel.jbang.init.pipe.yaml",
          "when": "false"
        },
        {
          "command": "kaoto.camel.jbang.export",
          "when": "false"
        },
        {
          "command": "kaoto.camel.jbang.export.folder",
          "when": "false"
        },
        {
          "command": "kaoto.camel.jbang.export.workspace",
          "when": "false"
        },
        {
          "command": "kaoto.integrations.run",
          "when": "false"
        },
        {
          "command": "kaoto.integrations.run.workspace",
          "when": "false"
        },
        {
          "command": "kaoto.integrations.run.all.workspaces",
          "when": "false"
        },
        {
          "command": "kaoto.integrations.run.folder",
          "when": "false"
        },
        {
          "command": "kaoto.integrations.kubernetes.run",
          "when": "false"
        },
        {
          "command": "kaoto.integrations.showSource",
          "when": "false"
        },
        {
          "command": "kaoto.integrations.delete",
          "when": "false"
        },
        {
          "command": "kaoto.integrations.updateDependencies",
          "when": "false"
        },
        {
          "command": "kaoto.deployments.stop",
          "when": "false"
        },
        {
          "command": "kaoto.deployments.logs",
          "when": "false"
        },
        {
          "command": "kaoto.deployments.route.start",
          "when": "false"
        },
        {
          "command": "kaoto.deployments.route.resume",
          "when": "false"
        },
        {
          "command": "kaoto.deployments.route.suspend",
          "when": "false"
        },
        {
          "command": "kaoto.deployments.route.stop",
          "when": "false"
        },
        {
          "command": "kaoto.undo",
          "when": "false"
        },
        {
          "command": "kaoto.redo",
          "when": "false"
        }
      ],
      "editor/title": [
        {
          "command": "kaoto.open.source",
          "when": "activeCustomEditorId == webviewEditorsKaoto",
          "group": "navigation@1"
        },
        {
          "command": "kaoto.close.source",
          "when": "(resourceFilename =~ /\\.(camel|kamelet|pipe).yaml$/ || resourceFilename =~ /\\-pipe.yaml$/ || resourceFilename =~ /\\.camel.xml$/) && activeCustomEditorId !== webviewEditorsKaoto",
          "group": "navigation@1"
        }
      ],
      "explorer/context": [
        {
          "command": "kaoto.open",
          "when": "resourceExtname == .yaml || resourceExtname == .yml || resourceExtname == .xml",
          "group": "1_belowOpenWith"
        }
      ],
      "view/title": [
        {
          "command": "kaoto.integrations.run.workspace",
          "group": "navigation@1",
          "when": "view == kaoto.integrations && !virtualWorkspace && kaoto.jbangAvailable && workspaceFolderCount == 1 && !kaoto.workspaceHasPomXml"
        },
        {
          "command": "kaoto.integrations.run.all.workspaces",
          "group": "navigation@1",
          "when": "view == kaoto.integrations && !virtualWorkspace && kaoto.jbangAvailable && workspaceFolderCount > 1"
        },
        {
          "submenu": "kaoto.new.file",
          "group": "navigation@2",
          "when": "view == kaoto.integrations && !virtualWorkspace && kaoto.jbangAvailable && workspaceFolderCount > 0"
        },
        {
          "command": "kaoto.camel.jbang.export.workspace",
          "group": "navigation@3",
          "when": "view == kaoto.integrations && !virtualWorkspace && kaoto.jbangAvailable && workspaceFolderCount == 1 && !kaoto.workspaceHasPomXml"
        },
        {
          "command": "kaoto.integrations.refresh",
          "group": "navigation@4",
          "when": "view == kaoto.integrations"
        },
        {
          "command": "kaoto.deployments.refresh",
          "group": "navigation",
          "when": "view == kaoto.deployments"
        }
      ],
      "view/item/context": [
        {
          "command": "kaoto.integrations.run.folder",
          "when": "view == kaoto.integrations && viewItem == folder && !virtualWorkspace && kaoto.jbangAvailable",
          "group": "inline@1"
        },
        {
          "command": "kaoto.camel.jbang.export.folder",
          "when": "view == kaoto.integrations && viewItem == folder && !virtualWorkspace && kaoto.jbangAvailable",
          "group": "inline@2"
        },
        {
          "command": "kaoto.integrations.showSource",
          "when": "view == kaoto.integrations && (viewItem == integration || viewItem == integration-standalone-child || viewItem == integration-maven-child || viewItem == file)",
          "group": "navigation@1"
        },
        {
          "command": "kaoto.integrations.delete",
          "when": "view == kaoto.integrations && (viewItem == integration || viewItem == integration-standalone-child || viewItem == integration-maven-child || viewItem == file || viewItem == folder || viewItem == folder-maven-child || viewItem == folder-maven-root)",
          "group": "navigation@2"
        },
        {
          "command": "kaoto.integrations.updateDependencies",
          "when": "view == kaoto.integrations && viewItem == integration-maven-child && !virtualWorkspace && kaoto.jbangAvailable",
          "group": "navigation@3"
        },
        {
          "command": "kaoto.integrations.run",
          "when": "view == kaoto.integrations && (viewItem == integration || viewItem == integration-standalone-child) && !virtualWorkspace && kaoto.jbangAvailable",
          "group": "inline@1"
        },
        {
          "command": "kaoto.integrations.kubernetes.run",
          "when": "view == kaoto.integrations && (viewItem == integration || viewItem == integration-standalone-child) && !virtualWorkspace && kaoto.jbangAvailable",
          "group": "inline@2"
        },
        {
          "command": "kaoto.camel.jbang.export",
          "when": "view == kaoto.integrations && (viewItem == integration || viewItem == integration-standalone-child) && !virtualWorkspace && kaoto.jbangAvailable",
          "group": "inline@3"
        },
        {
          "command": "kaoto.deployments.stop",
          "group": "inline@1",
          "when": "view == kaoto.deployments && viewItem == parent-localhost"
        },
        {
          "command": "kaoto.deployments.logs",
          "group": "inline@2",
          "when": "view == kaoto.deployments && viewItem == parent-localhost"
        },
        {
          "command": "kaoto.deployments.route.start",
          "group": "inline@1",
          "when": "view == kaoto.deployments && viewItem =~ /^.*startEnabled.*$/"
        },
        {
          "command": "kaoto.deployments.route.resume",
          "group": "inline@2",
          "when": "view == kaoto.deployments && viewItem =~ /^.*resumeEnabled.*$/"
        },
        {
          "command": "kaoto.deployments.route.suspend",
          "group": "inline@3",
          "when": "view == kaoto.deployments && viewItem =~ /^.*suspendEnabled.*$/"
        },
        {
          "command": "kaoto.deployments.route.stop",
          "group": "inline@4",
          "when": "view == kaoto.deployments && viewItem =~ /^.*stopEnabled.*$/"
        }
      ]
    },
    "submenus": [
      {
        "id": "kaoto.new.file",
        "label": "New File...",
        "icon": "$(new-file)"
      }
    ],
    "viewsWelcome": [
      {
        "view": "kaoto.integrations",
        "contents": "In order to start with Apache Camel, you can create a standalone Camel File.\n[Camel File...](command:kaoto.new.camel.file)\nTo learn more about Apache Camel [read docs](https://camel.apache.org/docs/).",
        "when": "!virtualWorkspace && kaoto.jbangAvailable && workspaceFolderCount > 0"
      },
      {
        "view": "kaoto.integrations",
        "contents": "You have not yet added a folder to the workspace.\n[Open Folder](command:vscode.openFolder)",
        "when": "workspaceFolderCount == 0"
      }
    ],
    "configuration": [
      {
        "id": "kaoto",
        "title": "Kaoto",
        "properties": {
          "kaoto.catalog.url": {
            "type": "string",
            "default": null,
            "markdownDescription": "URL to a Kaoto catalog. For instance `https://raw.githubusercontent.com/KaotoIO/camel-catalog/refs/heads/main/catalog/index.json `. Documentation to generate your own set of catalog is available [here](https://kaoto.io/docs/manual/09_generatingcatalog/). It requires to reopen the Kaoto editors to be effective.",
            "scope": "window",
            "order": 0
          },
          "kaoto.localKameletDirectories": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "additionalProperties": false,
            "markdownDescription": "List of local directories to be used for Kamelet files. It is used for every run by adding `--local-kamelet-dir` argument into `#kaoto.camelJbang.runArguments#` or `#kaoto.camelJbang.runFolderOrWorkspaceArguments#` settings.\n\n**Note**: The setting can be overridden by the `--local-kamelet-dir` argument set in mentioned run arguments settings.\n\nSupports common variables - `${workspaceFolder}`, `${workspaceFolderBasename}`, `${cwd}`",
            "default": [
              "."
            ],
            "scope": "window",
            "order": 1
          },
          "kaoto.camelVersion": {
            "type": "string",
            "markdownDescription": "Camel version used for internal Camel JBang CLI commands execution. As default Camel Version is used `#kaoto.camelJbang.version#`.",
            "order": 2
          },
          "kaoto.deployments.refresh.interval": {
            "type": "number",
            "markdownDescription": "Set default auto-refresh interval in milliseconds for a `Kaoto > Deployments` view. **Default recommended interval is 30s.**",
            "enum": [
              1000,
              5000,
              15000,
              30000,
              60000
            ],
            "enumItemLabels": [
              "1s",
              "5s",
              "15s",
              "30s",
              "60s"
            ],
            "default": 30000,
            "scope": "window",
            "order": 3
          },
          "kaoto.integrations.files.regexp": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "additionalProperties": false,
            "default": [
              "*.camel.yaml",
              "*.camel.xml",
              "*.kamelet.yaml",
              "*.pipe.yaml",
              "*-pipe.yaml",
              "*.properties",
              "*.xsl",
              "pom.xml"
            ],
            "markdownDescription": "Regular expression to filter files to be displayed in `Kaoto > Integrations` view.",
            "scope": "window",
            "order": 4
          },
          "redhat.telemetry.enabled": {
            "type": "boolean",
            "default": null,
            "markdownDescription": "Enable usage data and errors to be sent to Red Hat servers. Read our [privacy statement](https://developers.redhat.com/article/tool-data-collection).",
            "tags": [
              "telemetry"
            ],
            "scope": "window",
            "order": 5
          }
        }
      },
      {
        "id": "canvas",
        "title": "Canvas",
        "properties": {
          "kaoto.nodeLabel": {
            "type": "string",
            "default": "description",
            "markdownDescription": "Node label, which will be used for nodes in the canvas. Can be either `description` or `id`. If `description` is selected, it will be displayed only if it is available, otherwise `id` will be displayed by default. It requires to reopen the Kaoto editors to be effective.",
            "enum": [
              "description",
              "id"
            ],
            "scope": "window"
          },
          "kaoto.nodeToolbarTrigger": {
            "type": "string",
            "default": "onHover",
            "markdownDescription": "Choose when to open the Node toolbar. Can be either `onHover` or `onSelection`. If `onHover` is selected, the toolbar will be automatically open upon hovering a node, otherwise, it will be open when selecting a node. It requires to reopen the Kaoto editors to be effective.",
            "enum": [
              "onHover",
              "onSelection"
            ]
          },
          "kaoto.colorTheme": {
            "type": "string",
            "markdownDescription": "Choose the color theme for the UI. `auto` will follow VS Code theme, `light` and `dark` will set that theme regardless of VS Code theme. It requires to reopen the Kaoto editors to be effective.",
            "default": "auto",
            "enum": [
              "auto",
              "light",
              "dark"
            ]
          },
          "kaoto.enableDragAndDrop": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "Control whether to enable drag and drop feature. It requires to reopen the Kaoto editors to be effective.",
            "tags": [
              "preview"
            ]
          }
        }
      },
      {
        "id": "jbang",
        "title": "JBang",
        "properties": {
          "kaoto.camelJbang.version": {
            "type": "string",
            "markdownDescription": "Apache [Camel JBang](https://camel.apache.org/manual/camel-jbang.html) version used for an internal Camel JBang CLI calls. Requirements can differ between versions.\n\nIt is recommended to use `default` version to ensure all extension features works properly.",
            "default": "4.17.0",
            "order": 0
          },
          "kaoto.camelJbang.runArguments": {
            "type": "array",
            "uniqueItems": true,
            "items": {
              "type": "string"
            },
            "additionalProperties": false,
            "markdownDescription": "User defined arguments to be applied at every launch. In case of spaces, the values needs to be enclosed with quotes.\n\nFor possible values see [official docs](https://camel.apache.org/manual/camel-jbang.html#_creating_and_running_camel_routes).",
            "default": [
              "--dev",
              "--logging-level=info",
              "*.xsl"
            ],
            "order": 1
          },
          "kaoto.camelJbang.runFolderOrWorkspaceArguments": {
            "type": "array",
            "uniqueItems": true,
            "items": {
              "type": "string"
            },
            "additionalProperties": false,
            "markdownDescription": "User defined arguments to be applied at every launch of a folder or workspace. In case of spaces, the values needs to be enclosed with quotes.\n\nFor possible values see [official docs](https://camel.apache.org/manual/camel-jbang.html#_creating_and_running_camel_routes).",
            "default": [
              "--dev"
            ],
            "order": 2
          },
          "kaoto.camelJbang.redHatMavenRepository": {
            "type": "string",
            "markdownDescription": "Define Red Hat Maven Repository, which is used automatically in case the `#kaoto.camelVersion#` uses Red Hat productized Camel version (e.g. `4.8.0.redhat-00017`).",
            "default": "https://maven.repository.redhat.com/ga/",
            "order": 3
          },
          "kaoto.camelJbang.redHatMavenRepository.global": {
            "type": "boolean",
            "markdownDescription": "The `#repos` placeholder will be added by default to use also repositories defined in global Camel JBang configuration file.\n\n**Note**: The placeholder is available for versions `3.20.7/3.21` onwards.",
            "default": true,
            "order": 4
          },
          "kaoto.camelJbang.kubernetesRunArguments": {
            "type": "array",
            "uniqueItems": true,
            "items": {
              "type": "string"
            },
            "additionalProperties": false,
            "markdownDescription": "User defined arguments to be applied at every deploy (See [Camel JBang Kubernetes](https://camel.apache.org/manual/camel-jbang-kubernetes.html)). In case of spaces, the values needs to be enclosed with quotes.\n\n**Note**: Excluding `--camel-version` which is already being set in `#kaoto.camelVersion#`.\n\nBeware that `--disable-auto`, which is provided by default, requires Camel 4.11+.",
            "default": [
              "--cluster-type=openshift",
              "--disable-auto",
              "--dev"
            ],
            "order": 5
          }
        }
      },
      {
        "id": "maven",
        "title": "Maven Projects",
        "properties": {
          "kaoto.maven.dependenciesUpdate.onSave": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "Enable automatic update of Camel dependencies in pom.xml on save.",
            "order": 0
          },
          "kaoto.maven.camelJbang.exportProjectArguments": {
            "type": "array",
            "uniqueItems": true,
            "items": {
              "type": "string"
            },
            "additionalProperties": false,
            "markdownDescription": "User defined arguments to be applied at every export of a Maven project. In case of spaces, the values needs to be enclosed with quotes.\n\nFor possible values see [official docs](https://camel.apache.org/manual/camel-jbang.html#_creating_projects).",
            "default": [],
            "order": 1
          }
        }
      }
    ],
    "customEditors": [
      {
        "viewType": "webviewEditorsKaoto",
        "displayName": "Kaoto Editor",
        "selector": [
          {
            "filenamePattern": "*{.camel,.kamelet,.pipe,-pipe}.{yaml,yml}"

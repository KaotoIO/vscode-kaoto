{
  "name": "vscode-kaoto",
  "publisher": "redhat",
  "displayName": "Kaoto",
  "description": "Kaoto - No Code and low code Integration editor",
  "version": "0.1.0",
  "preview": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaotoIO/vscode-kaoto.git"
  },
  "bugs": "https://github.com/KaotoIO/vscode-kaoto/issues",
  "homepage": "https://github.com/KaotoIO/vscode-kaoto",
  "author": {
    "name": "Aur√©lien Pupier",
    "email": "apupier@redhat.com"
  },
  "scripts": {
    "lint": "npm eslint ./src --ext .ts,.tsx",
    "build:prod": "rimraf dist && webpack && npm pack:prod",
    "build:dev": "rimraf dist && webpack --env dev",
    "pack:prod": "vsce package --githubBranch main  --yarn -o ./dist/vscode_kaoto_$npm_package_version.vsix",
    "compile": "webpack",
    "watch": "webpack",
    "run:webmode": "npm vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --version=stable"
  },
  "dependencies": {
    "@kie-tools-core/backend": "^0.23.0",
    "@kie-tools-core/editor": "^0.23.0",
    "@kie-tools-core/i18n": "^0.23.0",
    "@kie-tools-core/monaco-editor": "^0.23.0",
    "@kie-tools-core/patternfly-base": "^0.23.0",
    "@kie-tools-core/vscode-extension": "^0.23.0",
    "@kie-tools-core/workspace": "^0.23.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "kaoto-ui": "0.1.0"
  },
  "icon": "icon.png",
  "main": "./dist/extension/extension.js",
  "browser": "./dist/extension/extensionWeb.js",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Kaoto",
    "Apache Camel",
    "Low Code Integration",
    "No Code Integration"
  ],
  "activationEvents": [
    "onCustomEditor:kieKogitoWebviewEditorsKaoto"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    }
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "kieKogitoWebviewEditorsKaoto",
        "displayName": "Kaoto Editor",
        "selector": [
          {
            "filenamePattern": "*.{kaoto.yaml,kaoto.yml}"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "kaoto-yaml",
        "extensions": [
          ".kaoto.yml",
          ".kaoto.yaml"
        ],
        "aliases": [
          "Kaoto (YAML)",
          "kaoto.yaml",
          "kaoto.yml"
        ]
      }
    ]
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "npm-link-shared": "^0.5.6",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
